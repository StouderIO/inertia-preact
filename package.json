{
  "name": "@stouder-io/inertia-preact",
  "version": "1.0.14",
  "license": "MIT",
  "description": "The Preact adapter for Inertia.js",
  "author": "Xavier Stouder <xavier@stouder.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/StouderIO/inertia-preact.git"
  },
  "bugs": {
    "url": "https://github.com/StouderIO/inertia-preact/issues"
  },
  "files": [
    "dist",
    "types"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./types/server.d.ts",
      "import": "./dist/server.esm.js",
      "require": "./dist/server.js"
    }
  },
  "typesVersions": {
    "*": {
      "server": [
        "types/server.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "node build.js --watch",
    "build": "npm run clean && node build.js && tsc --emitDeclarationOnly",
    "clean": "rimraf types && rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "esbuild": "^0.16.13",
    "esbuild-node-externals": "^1.12.0",
    "eslint": "^8.56.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^25.7.0",
    "preact": "^10.19.3",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "preact": "^10.19.3"
  },
  "dependencies": {
    "@inertiajs/core": "1.0.14",
    "lodash.isequal": "^4.5.0",
    "preact-render-to-string": "^6.3.1"
  },
  "eslintConfig": {
    "env": {
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "preact",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": ["error"],
      "no-redeclare": "off",
      "@typescript-eslint/no-redeclare": ["error"],
      "react-hooks/rules-of-hooks": "off"
    },
    "settings": {
      "jest": {
        "version": 29
      }
    }
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}